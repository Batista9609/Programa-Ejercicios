        static Double Gasto_calorico(Double pesoL, Double alturaL, Int32 edadL, String intensidadL, String SexoL)
        {
            Double Gastocal;
            if (intensidadL == "Baja")
            {
                if (SexoL == "femenino")
                {

                    Gastocal = ((655 + (9.6 * pesoL) + (1.8 * alturaL) - (4.7 * edadL))) * 1.3;
                    return Gastocal;
                }
                else
                {
                    Gastocal = ((66 + (13.7 * pesoL) + (5 * alturaL) - (6.75 * edadL))) * 1.3;
                    return Gastocal;
                }
            }

            if (intensidadL == "Media")
            {
                if (SexoL == "femenino")
                {

                    Gastocal = ((655 + (9.6 * pesoL) + (1.8 * alturaL) - (4.7 * edadL))) * 1.4;
                    return Gastocal;
                }
                else
                {
                    Gastocal = ((66 + (13.7 * pesoL) + (5 * alturaL) - (6.75 * edadL))) * 1.4;
                    return Gastocal;
                }
            }

            // Intensidad Alta
            {
                if (SexoL == "femenino")
                {

                    Gastocal = ((655 + (9.6 * pesoL) + (1.8 * alturaL) - (4.7 * edadL))) * 1.5;
                    return Gastocal;
                }
                else
                {
                    Gastocal = ((66 + (13.7 * pesoL) + (5 * alturaL) - (6.75 * edadL))) * 1.5;
                    return Gastocal;
                }
            }
        }
        static Double ProgresionAumento(Double pesoL, Double pesoD, Double alturaL, Int32 edadL, String intensidadL, String SexoL)
        {
            int Conversion, Conversion2;

            Conversion = Convert.ToInt32((Gasto_calorico(pesoL, alturaL, edadL, intensidadL, SexoL) - ConsumoAumento(pesoL, alturaL, edadL, intensidadL, SexoL)));


            Double totalR = ((pesoD * 7000) - (pesoL * 7000));
            int semana = Conversion * 7;
            Conversion2 = Convert.ToInt32(totalR);

            int semanas = Conversion2 / semana;

            return semanas;
        }
        static Double ProgresionConsumo(Double pesoL, Double pesoD)
        {

            int Conversion2;
            int tot;
            tot = 500;


            Double totalR = ((pesoL * 7000) - (pesoD * 7000));
            int semana = tot * 7;
            Conversion2 = Convert.ToInt32(totalR);

            int semanas = Conversion2 / semana;

            return semanas;
        }
        static Double ProgresioConsumoFecha(Double pesoL, Double pesoD)
        {

            Double dias = ProgresionConsumo(pesoL, pesoD) * 7;
            return dias;
        }
        static Double ProgresionAumentoFecha(Double pesoL, Double pesoD, Double alturaL, Int32 edadL, String intensidadL, String SexoL)
        {

            Double dias = Convert.ToInt32(ProgresionAumento(pesoL, pesoD, alturaL, edadL, intensidadL, SexoL)) * 7;
            return dias;
        }
